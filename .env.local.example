NEXT_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Your Skin Analysis API Endpoint (Replace with your actual API)
NEXT_PUBLIC_SKIN_ANALYSIS_API=https://your-skin-analysis-api.com/analyze